issn = {0031899X},
journal = {Physical Review},
number = {4},
pages = {1068--1083},
title = {{Electron spin resonance experiments on donors in silicon. III. investigation of excited states by the application of uniaxial stress and their importance in relaxation processes}},
volume = {124},
year = {1961}
}
