@article{Morton2008a,
abstract = {A recent article by Yang and Hoffman [T.-C. Yang, B.M. Hoffman, J. Magn. Reson., 181 (2006) 280] presents a 'Davies/Hahn ENDOR multi-sequence' in which the $\alpha$ and $\beta$ peaks of an electron-nuclear double resonance (ENDOR) spectrum can be distinguished. This represents one instance of a family of ENDOR sequences which have no initial microwave inversion pulse, and which can reveal information about nuclear relaxation rates and the signs of hyperfine coupling constants. Here we discuss the more general set of such sequences, which we refer to as Saturated Pulsed ENDOR, and show how signal sensitivity can be optimised within the context of this new technique. Through simulations, we compare its performance to other techniques based on Davies ENDOR, and experimentally illustrate its properties using the non-heme Fe enzyme anthranilate dioxygenase AntDO. Finally, we suggest a protocol for extracting both the magnitude and sign of the hyperfine tensor using a combination of ENDOR techniques. {\textcopyright} 2008 Elsevier Inc. All rights reserved.},
