@article{Craik2016,
abstract = {Individual addressing of qubits is essential for scalable quantum computation. Spatial addressing allows unlimited numbers of qubits to share the same frequency, whilst enabling arbitrary parallel operations. We demonstrate addressing of long-lived {\$}{\^{}}{\{}43{\}}\backslashtext{\{}Ca{\}}{\^{}}+{\$} "atomic clock" qubits held in separate zones of a microfabricated surface trap with integrated microwave electrodes. By coherently cancelling the microwave field in one zone we measure a ratio of Rabi frequencies between addressed and non-addressed qubits of up to 1400, implying an addressing error of {\$}1.3\backslashtimes 10{\^{}}{\{}-6{\}}{\$}. Off-resonant excitation prevents this error level being directly demonstrated, but we also show polarization control of the microwave field with error {\$}2\backslashtimes 10{\^{}}{\{}-5{\}}{\$}, sufficient to suppress off-resonant excitation out of the qubit states to the {\$}\backslashsim 10{\^{}}{\{}-9{\}}{\$} level. Such polarization control could enable fast microwave operations.},
