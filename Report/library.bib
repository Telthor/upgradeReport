doi = {10.1038/ncomms2773},
eprint = {arXiv:1211.2217v3},
file = {:Users/davidwise/Library/Application Support/Mendeley Desktop/Downloaded/Nickerson, Li, Benjamin - 2013 - Topological quantum computing with a very noisy network and local error rates approaching one percent.pdf:pdf},
isbn = {2041-1723},
