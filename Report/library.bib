issn = {0031899X},
journal = {Physical Review},
number = {5},
pages = {1219--1244},
title = {{Electron spin resonance experiments on donors in silicon. I. Electronic structure of donors by the electron nuclear double resonance technique}},
volume = {114},
year = {1959}
}
