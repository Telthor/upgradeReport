issn = {1748-3395},
journal = {Nature nanotechnology},
keywords = {Diamond,Diamond: chemistry,Equipment Design,Molecular Imaging,Molecular Imaging: instrumentation,Molecular Imaging: methods,Nanotechnology,Nanotechnology: instrumentation,Nanotechnology: methods,Nitrogen,Nitrogen: chemistry},
