title = {{Heralded entanglement between solid-state qubits separated by three metres}},
url = {http://www.nature.com/nature/journal/v497/n7447/full/nature12016.html{\%}5Cnhttp://www.nature.com/nature/journal/v497/n7447/pdf/nature12016.pdf},
