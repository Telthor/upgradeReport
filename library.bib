@article{Romach2019,
abstract = {Understanding the physical origin of noise affecting quantum systems is important for nearly every quantum application. Quantum-noise spectroscopy has been used in various quantum systems, such as superconducting qubits, nitrogen-vacancy centers, and trapped ions. Traditional spectroscopy methods are usually efficient in measuring noise spectra with mostly monotonically decaying contributions. However, there are important scenarios in which the noise spectrum is broadband and nonmonotonous, thus posing a challenge to existing noise-spectroscopy schemes. Here we compare several methods for noise spectroscopy: spectral decomposition based on the Carr-Purcell-Meiboom-Gill sequence, the recently presented dynamic sensitivity control (DYSCO) sequence, and a modified DYSCO sequence with a Gaussian envelope (gDYSCO). The performance of the sequences is quantified by analytic and numeric determination of the frequency resolution, bandwidth, and sensitivity, revealing a supremacy of gDYSCO to reconstruct nontrivial features. Using an ensemble of nitrogen-vacancy centers in diamond coupled to a high-density C13-nuclear-spin environment, we experimentally confirm our findings. The combination of the schemes presented offers potential to record high-quality noise spectra as a prerequisite to generate quantum systems unlimited by their spin-bath environment.},
author = {Romach, Y. and Lazariev, A. and Avrahami, I. and Klei{\ss}ler, F. and Arroyo-Camejo, S. and Bar-Gill, N.},
doi = {10.1103/PhysRevApplied.11.014064},
file = {:Users/davidwise/Google Drive/UCL/Mendeley Library/Romach et al/Romach et al.{\_}2019{\_}Measuring Environmental Quantum Noise Exhibiting a Nonmonotonic Spectral Shape.pdf:pdf},
issn = {23317019},
journal = {Physical Review Applied},
keywords = {doi:10.1103/PhysRevApplied.11.014064 url:https://d},
number = {1},
pages = {1},
publisher = {American Physical Society},
title = {{Measuring Environmental Quantum Noise Exhibiting a Nonmonotonic Spectral Shape}},
url = {https://doi.org/10.1103/PhysRevApplied.11.014064},
volume = {11},
year = {2019}
}
@phdthesis{Burrell2010,
abstract = {This thesis describes experimental demonstrations of high-fidelity$\backslash$nreadout of trapped$\backslash$n$\backslash$nion quantum bits (�qubits�) for quantum information processing.$\backslash$n$\backslash$nWe present direct single-shot measurement of an �optical� qubit stored$\backslash$nin a single$\backslash$n$\backslash$n40Ca+ ion by the process of resonance fluorescence with a fidelity$\backslash$nof 99.991(1){\%} (surpassing$\backslash$n$\backslash$nthe level necessary for fault-tolerant quantum computation). A time-resolved$\backslash$n$\backslash$nmaximum likelihood method is used to discriminate efficiently between$\backslash$nthe two qubit$\backslash$n$\backslash$nstates based on photon-counting information, even in the presence$\backslash$nof qubit decay from$\backslash$n$\backslash$none state to the other. It also screens out errors due to cosmic ray$\backslash$nevents in the detector,$\backslash$n$\backslash$na phenomenon investigated in this work. An adaptive method allows$\backslash$nthe 99.99{\%}$\backslash$n$\backslash$nlevel to be reached in 145 mus average detection time. The readout$\backslash$nfidelity is asymmetric:$\backslash$n$\backslash$n99.9998{\%} is possible for the �bright� qubit state, while retaining$\backslash$n99.98{\%} for the �dark�$\backslash$n$\backslash$nstate. This asymmetry could be exploited in quantum error correction$\backslash$n(by encoding the$\backslash$n$\backslash$n�no-error� syndrome of the ancilla qubits in the �bright� state),$\backslash$nas could the likelihood$\backslash$n$\backslash$nvalues computed (which quantify confidence in the measurement outcome).$\backslash$n$\backslash$nWe then extend the work to parallel readout of a four-ion string using$\backslash$na CCD camera$\backslash$n$\backslash$nand achieve the same 99.99{\%} net fidelity, limited by qubit decay in$\backslash$nthe 400 micros exposure$\backslash$n$\backslash$ntime. The behaviour of the camera is characterised by fitting experimental$\backslash$ndata with a$\backslash$n$\backslash$nmodel. The additional readout error due to cross-talk between ion$\backslash$nimages on the CCD$\backslash$n$\backslash$nis measured in an experiment designed to remove the effect of qubit$\backslash$ndecay; a spatial$\backslash$n$\backslash$nmaximum likelihood technique is used to reduce this error to only$\backslash$n0.2(1) 10-4 per$\backslash$n$\backslash$nqubit, despite the presence of 4{\%} optical cross-talk between neighbouring$\backslash$nqubits.$\backslash$n$\backslash$nStudies of the cross-talk indicate that the readout method would scale$\backslash$nwith negligible$\backslash$n$\backslash$nloss of fidelity to parallel readout of 10 000 qubits with a readout$\backslash$ntime of 3 micros per$\backslash$n$\backslash$nqubit.$\backslash$n$\backslash$nMonte-Carlo simulations of the readout process are presented for comparison$\backslash$nwith$\backslash$n$\backslash$nexperimental data; these are also used to explore the parameter space$\backslash$nassociated with$\backslash$n$\backslash$nfluorescence detection and to optimise experimental and analysis parameters.$\backslash$n$\backslash$nApplications of the analysis methods to readout of other atomic and$\backslash$nsolid-state$\backslash$n$\backslash$nqubits are discussed.},
