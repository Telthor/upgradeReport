@article{Feher1959,
abstract = {The different relaxation processes that connect the four energy levels in phosphorus doped silicon have been investigated experimentally. The relaxation time Ts ($\Delta$ms=±1, $\Delta$mI=0) was found to be independent of phosphorus concentration below ∼1016 P/cm3. Its value at 3200 oersteds and 1.25°K was ∼3×10+3 seconds and varied as 1/T for 1.3°K{\textless}T{\textless}2°K and as 1/T7 for 2.5°K{\textless}T{\textless}4.2°K. The magnetic field dependence in the 1/T region suggests a direct phonon process. In the 1/T7 region Ts was independent of the magnetic field between 3000 and 8000 oersteds. Above a concentration of 1016 P/cm3 Ts varied rapidly with donor concentration, dropping to 10-4 seconds at 3×1017 P/cm3. In this concentration dependent region Ts was independent of the magnetic field but depended on the number of acceptors present. None of the Ts mechanisms can at present be accounted for by the theories of Pines, Bardeen, and Slichter, and Abrahams. The relaxation time Tx ($\Delta$ms=±1, $\Delta$mI=∓1) was ∼30 hours at 3000 oersteds and ∼5 hours at 8000 oersteds in fair agreement with the theory of PBS. The relaxation time TN ($\Delta$mI=±1, $\Delta$ms=0) at 1.25°K exceeded 10 hours.},
author = {Feher, G.},
doi = {10.1103/PhysRev.114.1219},
file = {:Users/davidwise/Google Drive/UCL/Mendeley Library/Feher/Feher{\_}1959{\_}Electron spin resonance experiments on donors in silicon. I. Electronic structure of donors by the electron nuclear double re.pdf:pdf},
isbn = {0031-899X$\backslash$n1536-6065},
issn = {0031899X},
journal = {Physical Review},
number = {5},
pages = {1219--1244},
title = {{Electron spin resonance experiments on donors in silicon. I. Electronic structure of donors by the electron nuclear double resonance technique}},
volume = {114},
year = {1959}
}
