publisher = {Nature Publishing Group},
title = {{Error-corrected quantum annealing with hundreds of qubits}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24500027{\%}5Cnhttp://www.nature.com/ncomms/2014/140206/ncomms4243/full/ncomms4243.html{\%}5Cnhttp://www.nature.com/doifinder/10.1038/ncomms4243},
