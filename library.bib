eprint = {1803.05538},
file = {:Users/davidwise/Google Drive/UCL/Mendeley Library/Norris et al/Norris et al.{\_}2018{\_}Optimally band-limited spectroscopy of control noise using a qubit sensor.pdf:pdf},
