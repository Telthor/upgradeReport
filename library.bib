issn = {1745-2473},
journal = {Nature Physics},
number = {12},
pages = {929--930},
title = {{Quantum computing: Snapshots of diamond spins}},
url = {http://www.nature.com/doifinder/10.1038/nphys2148},
volume = {7},
year = {2011}
}
